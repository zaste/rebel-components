            "step_control": { "recommended": true, "description": "Define value increments" },
            "range_slider": { "optional": true, "description": "Select value ranges" }
          },
          "aria_requirements": ["role=slider", "aria-valuemin", "aria-valuemax", "aria-valuenow"],
          "keyboard_support": ["ArrowLeft", "ArrowRight", "Home", "End", "PageUp", "PageDown"],
          "use_cases": ["price ranges", "volume controls", "filters", "configuration"]
        }
      },
      
      "design_principles": [
        "Clear field labeling and purpose",
        "Immediate validation feedback",
        "Forgiving error recovery",
        "Logical tab order and grouping",
        "Accessible to all input methods"
      ]
    },
    
    "feedback": {
      "description": "Status Communication - Informs users about system state and actions",
      "verb": "communicate",
      "core_behavior": "convey_status",
      "state_pattern": "visible/hidden/persistent",
      "common_events": ["show", "dismiss", "timeout", "update"],
      
      "patterns": {
        "toast": {
          "description": "Temporary notification messages",
          "capability_level": "interactive",
          "typical_size": "3.9kb",
          "satisfaction_rate": 0.91,
          "features": {
            "auto_dismiss": { "essential": true, "description": "Disappear automatically" },
            "manual_dismiss": { "essential": true, "description": "User can close" },
            "position_control": { "recommended": true, "description": "Configurable position" },
            "stacking": { "optional": true, "description": "Multiple toasts" }
          },
          "aria_requirements": ["role=alert", "aria-live", "aria-atomic"],
          "keyboard_support": ["Escape", "Tab"],
          "use_cases": ["success messages", "error notifications", "info updates", "confirmations"]
        },
        
