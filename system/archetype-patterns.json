          "typical_size": "2.5kb",
          "satisfaction_rate": 0.91,
          "features": {
            "visual_feedback": { "essential": true, "description": "Clear on/off states" },
            "smooth_animation": { "recommended": true, "description": "Satisfying transitions" },
