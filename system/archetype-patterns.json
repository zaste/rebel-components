{
  "archetypes": {
    "disclosure": {
      "description": "Show/Hide behaviors - Controls visibility and revelation of content",
      "verb": "reveal",
      "core_behavior": "toggle_visibility",
      "state_pattern": "open/closed",
      "common_events": ["show", "hide", "toggle"],
      
      "patterns": {
        "modal": {
          "description": "Overlay dialog that appears above main content",
          "capability_level": "interactive",
          "typical_size": "6.8kb",
          "satisfaction_rate": 0.92,
          "features": {
            "backdrop": { "essential": true, "description": "Dark overlay behind modal" },
            "focus_trap": { "essential": true, "description": "Keeps focus within modal" },
            "escape_close": { "essential": true, "description": "Close on escape key" },
            "body_scroll_lock": { "recommended": true, "description": "Prevent background scroll" }
          },
          "aria_requirements": ["role=dialog", "aria-labelledby", "aria-describedby"],
          "keyboard_support": ["Escape", "Tab", "Shift+Tab"],
          "use_cases": ["contact forms", "confirmations", "image galleries", "login forms"]
        },
        
        "accordion": {
          "description": "Collapsible content panels",
          "capability_level": "interactive",
          "typical_size": "3.2kb",
          "satisfaction_rate": 0.88,
          "features": {
            "smooth_animation": { "essential": true, "description": "Animated open/close" },
            "multiple_open": { "optional": true, "description": "Allow multiple panels open" },
            "keyboard_navigation": { "essential": true, "description": "Arrow key navigation" }
          },
          "aria_requirements": ["role=button", "aria-expanded", "aria-controls"],
          "keyboard_support": ["Enter", "Space", "ArrowDown", "ArrowUp"],
          "use_cases": ["FAQs", "menu sections", "content organization", "settings panels"]
        },
        
        "dropdown": {
          "description": "Menu that appears on trigger activation",
          "capability_level": "interactive",
          "typical_size": "4.5kb",
          "satisfaction_rate": 0.87,
          "features": {
            "auto_positioning": { "essential": true, "description": "Smart position calculation" },
            "click_outside_close": { "essential": true, "description": "Close when clicking outside" },
            "keyboard_navigation": { "essential": true, "description": "Arrow key navigation" }
          },
          "aria_requirements": ["role=menu", "aria-haspopup", "aria-expanded"],
          "keyboard_support": ["Enter", "Space", "ArrowDown", "ArrowUp", "Escape"],
          "use_cases": ["navigation menus", "action menus", "select alternatives", "context menus"]
        },
        
        "tooltip": {
          "description": "Small informational popup on hover/focus",
          "capability_level": "interactive",
          "typical_size": "2.1kb",
          "satisfaction_rate": 0.91,
          "features": {
            "smart_positioning": { "essential": true, "description": "Avoid viewport edges" },
            "hover_delay": { "recommended": true, "description": "Delay before showing" },
            "focus_support": { "essential": true, "description": "Show on keyboard focus" }
          },
          "aria_requirements": ["role=tooltip", "aria-describedby"],
          "keyboard_support": ["Escape"],
          "use_cases": ["help text", "additional info", "form field hints", "icon explanations"]
        },
        
        "popover": {
          "description": "Contextual information bubble",
          "capability_level": "interactive",
          "typical_size": "3.8kb",
          "satisfaction_rate": 0.89,
          "features": {
            "rich_content": { "essential": true, "description": "Support for complex content" },
            "arrow_pointer": { "recommended": true, "description": "Visual pointer to trigger" },
            "manual_trigger": { "essential": true, "description": "Click to show/hide" }
          },
          "aria_requirements": ["role=dialog", "aria-labelledby"],
          "keyboard_support": ["Escape", "Tab"],
          "use_cases": ["detailed info", "mini forms", "rich tooltips", "contextual actions"]
        }
      },
      
      "design_principles": [
        "Clear visual hierarchy between trigger and content",
        "Obvious entry and exit affordances",
        "Respect user attention and focus",
        "Provide clear state indicators",
        "Support both mouse and keyboard interaction"
      ]
    },
    
    "navigation": {
      "description": "Switch Views - Controls movement between different content areas",
      "verb": "navigate",
      "core_behavior": "change_active_view",
      "state_pattern": "active/inactive",
      "common_events": ["activate", "deactivate", "navigate"],
      
      "patterns": {
        "tabs": {
          "description": "Horizontal or vertical tab interface",
          "capability_level": "interactive",
          "typical_size": "4.1kb",
          "satisfaction_rate": 0.90,
          "features": {
            "roving_tabindex": { "essential": true, "description": "Keyboard navigation pattern" },
            "orientation_support": { "recommended": true, "description": "Horizontal/vertical layouts" },
            "lazy_loading": { "optional": true, "description": "Load panels on demand" }
          },
          "aria_requirements": ["role=tablist", "role=tab", "role=tabpanel", "aria-selected"],
          "keyboard_support": ["ArrowLeft", "ArrowRight", "ArrowUp", "ArrowDown", "Home", "End"],
          "use_cases": ["content sections", "settings panels", "data views", "multi-step forms"]
        },
        
        "sidebar": {
          "description": "Collapsible side navigation",
          "capability_level": "interactive",
          "typical_size": "5.3kb",
          "satisfaction_rate": 0.86,
          "features": {
            "responsive_collapse": { "essential": true, "description": "Mobile-friendly collapse" },
            "overlay_mode": { "recommended": true, "description": "Mobile overlay behavior" },
            "persistent_state": { "optional": true, "description": "Remember collapsed state" }
          },
          "aria_requirements": ["role=navigation", "aria-label", "aria-expanded"],
          "keyboard_support": ["Tab", "Enter", "Space"],
          "use_cases": ["admin panels", "app navigation", "content hierarchy", "filter panels"]
        },
        
        "stepper": {
          "description": "Multi-step process navigation",
          "capability_level": "stateful",
          "typical_size": "6.7kb",
          "satisfaction_rate": 0.88,
          "features": {
            "progress_indication": { "essential": true, "description": "Visual progress tracking" },
            "step_validation": { "essential": true, "description": "Validate before proceeding" },
            "non_linear_navigation": { "optional": true, "description": "Jump to any valid step" }
          },
          "aria_requirements": ["role=progressbar", "aria-valuenow", "aria-valuemax"],
          "keyboard_support": ["Tab", "Enter", "ArrowLeft", "ArrowRight"],
          "use_cases": ["wizards", "checkouts", "onboarding", "multi-part forms"]
        },
        
        "pagination": {
          "description": "Navigate through pages of content",
          "capability_level": "interactive",
          "typical_size": "3.4kb",
          "satisfaction_rate": 0.87,
          "features": {
            "page_size_control": { "optional": true, "description": "Items per page selector" },
            "jump_to_page": { "optional": true, "description": "Direct page input" },
            "infinite_scroll": { "alternative": true, "description": "Alternative pattern" }
          },
          "aria_requirements": ["role=navigation", "aria-label", "aria-current"],
          "keyboard_support": ["Tab", "Enter", "Space"],
          "use_cases": ["search results", "data tables", "image galleries", "blog posts"]
        },
        
        "breadcrumb": {
          "description": "Hierarchical navigation trail",
          "capability_level": "static",
          "typical_size": "1.9kb",
          "satisfaction_rate": 0.84,
          "features": {
            "structured_data": { "recommended": true, "description": "Schema.org markup" },
            "truncation": { "optional": true, "description": "Handle long paths" },
            "dynamic_updates": { "optional": true, "description": "Update on navigation" }
          },
          "aria_requirements": ["role=navigation", "aria-label"],
          "keyboard_support": ["Tab", "Enter"],
          "use_cases": ["site navigation", "file browsers", "category paths", "process flows"]
        }
      },
      
      "design_principles": [
        "Clear indication of current location",
        "Obvious navigation affordances",
        "Consistent interaction patterns",
        "Logical information architecture",
        "Efficient keyboard navigation"
      ]
    },
    
    "collection": {
      "description": "Display Data - Presents and organizes collections of information",
      "verb": "display",
      "core_behavior": "organize_information",
      "state_pattern": "filtered/sorted/grouped",
      "common_events": ["filter", "sort", "select", "paginate"],
      
      "patterns": {
        "filter": {
          "description": "Advanced filtering interface - GOLD STANDARD",
          "capability_level": "stateful",
          "typical_size": "8.3kb",
          "satisfaction_rate": 0.94,
          "features": {
            "debounced_search": { "essential": true, "description": "Performant text search" },
            "tag_filters": { "essential": true, "description": "Multi-select category filters" },
            "state_persistence": { "recommended": true, "description": "Remember filter state" },
            "clear_all": { "essential": true, "description": "Reset all filters" }
          },
          "aria_requirements": ["role=search", "aria-live", "aria-atomic"],
          "keyboard_support": ["Tab", "Enter", "Space", "ArrowDown", "ArrowUp"],
          "use_cases": ["product catalogs", "search interfaces", "data exploration", "content discovery"]
        },
        
        "table": {
          "description": "Structured data presentation",
          "capability_level": "stateful",
          "typical_size": "9.2kb",
          "satisfaction_rate": 0.86,
          "features": {
            "sortable_columns": { "essential": true, "description": "Click to sort data" },
            "responsive_design": { "essential": true, "description": "Mobile-friendly layout" },
            "row_selection": { "optional": true, "description": "Select multiple rows" },
            "inline_editing": { "advanced": true, "description": "Edit cells directly" }
          },
          "aria_requirements": ["role=table", "role=columnheader", "aria-sort"],
          "keyboard_support": ["Tab", "Enter", "Space", "ArrowKeys"],
          "use_cases": ["data grids", "comparison tables", "admin interfaces", "reports"]
        },
        
        "gallery": {
          "description": "Visual content showcase",
          "capability_level": "interactive",
          "typical_size": "7.1kb",
          "satisfaction_rate": 0.89,
          "features": {
            "lightbox_mode": { "essential": true, "description": "Full-screen viewing" },
            "thumbnail_navigation": { "essential": true, "description": "Grid view navigation" },
            "lazy_loading": { "recommended": true, "description": "Performance optimization" },
            "keyboard_navigation": { "essential": true, "description": "Arrow key navigation" }
          },
          "aria_requirements": ["role=img", "aria-label", "aria-describedby"],
          "keyboard_support": ["ArrowLeft", "ArrowRight", "Enter", "Escape"],
