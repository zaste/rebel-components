        "kanban": {
          "description": "Card-based workflow visualization",
          "capability_level": "connected",
          "typical_size": "12.1kb",
          "satisfaction_rate": 0.91,
          "features": {
            "drag_and_drop": { "essential": true, "description": "Move cards between columns" },
            "card_editing": { "essential": true, "description": "Edit card content" },
            "column_management": { "optional": true, "description": "Add/remove columns" },
            "progress_tracking": { "recommended": true, "description": "Visual progress indicators" }
          },
          "aria_requirements": ["role=application", "aria-grabbed", "aria-dropeffect"],
          "keyboard_support": ["Tab", "Enter", "Space", "ArrowKeys", "Delete"],
          "use_cases": ["project management", "task boards", "workflow tracking", "content organization"]
        },
        
        "list": {
          "description": "Simple item enumeration",
          "capability_level": "static",
          "typical_size": "2.8kb",
          "satisfaction_rate": 0.85,
          "features": {
            "virtual_scrolling": { "advanced": true, "description": "Handle large lists" },
            "item_actions": { "optional": true, "description": "Contextual actions" },
            "grouping": { "optional": true, "description": "Categorize items" }
          },
          "aria_requirements": ["role=list", "role=listitem"],
          "keyboard_support": ["Tab", "ArrowUp", "ArrowDown"],
          "use_cases": ["navigation menus", "contact lists", "simple catalogs", "content indices"]
        }
      },
      
      "design_principles": [
        "Scannable information hierarchy",
        "Consistent item formatting",
        "Efficient filtering and sorting",
        "Clear visual grouping",
        "Performance with large datasets"
      ]
    },
    
    "input": {
      "description": "User Entry - Collects and validates user information",
      "verb": "collect",
      "core_behavior": "capture_data",
      "state_pattern": "valid/invalid/pending",
      "common_events": ["input", "validate", "submit", "change"],
      
      "patterns": {
        "form": {
          "description": "Complete data collection interface",
          "capability_level": "stateful",
          "typical_size": "7.9kb",
          "satisfaction_rate": 0.85,
          "features": {
            "real_time_validation": { "essential": true, "description": "Validate as user types" },
            "error_display": { "essential": true, "description": "Clear error messaging" },
            "progress_indication": { "recommended": true, "description": "Show completion progress" },
            "auto_save": { "optional": true, "description": "Save draft automatically" }
          },
          "aria_requirements": ["role=form", "aria-invalid", "aria-describedby"],
          "keyboard_support": ["Tab", "Enter", "Escape"],
          "use_cases": ["contact forms", "registration", "surveys", "data entry"]
        },
        
        "select": {
          "description": "Choose from predefined options",
          "capability_level": "interactive",
          "typical_size": "3.7kb",
          "satisfaction_rate": 0.86,
          "features": {
            "searchable": { "recommended": true, "description": "Filter options by typing" },
            "multi_select": { "optional": true, "description": "Select multiple options" },
            "custom_options": { "advanced": true, "description": "Add new options" }
          },
          "aria_requirements": ["role=combobox", "aria-expanded", "aria-activedescendant"],
          "keyboard_support": ["ArrowUp", "ArrowDown", "Enter", "Escape", "Type to search"],
