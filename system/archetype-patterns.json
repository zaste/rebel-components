          },
          "aria_requirements": ["role=alert", "aria-labelledby"],
          "keyboard_support": ["Tab", "Enter"],
          "use_cases": ["form errors", "system status", "warnings", "important announcements"]
        },
        
        "progress": {
          "description": "Task completion indicators",
          "capability_level": "interactive",
          "typical_size": "3.1kb",
          "satisfaction_rate": 0.86,
          "features": {
            "percentage_display": { "essential": true, "description": "Show completion percentage" },
            "indeterminate_mode": { "essential": true, "description": "Unknown duration tasks" },
            "cancellation": { "optional": true, "description": "Allow task cancellation" }
          },
          "aria_requirements": ["role=progressbar", "aria-valuenow", "aria-valuemax"],
          "keyboard_support": ["Escape (if cancellable)"],
          "use_cases": ["file uploads", "form submission", "data loading", "installation progress"]
        },
        
        "skeleton": {
          "description": "Loading state placeholder",
          "capability_level": "static",
          "typical_size": "1.7kb",
          "satisfaction_rate": 0.88,
          "features": {
            "content_shape": { "essential": true, "description": "Match final content layout" },
            "animation": { "recommended": true, "description": "Subtle loading animation" },
            "progressive_reveal": { "optional": true, "description": "Load sections incrementally" }
          },
          "aria_requirements": ["aria-busy", "aria-label"],
          "keyboard_support": [],
          "use_cases": ["content loading", "image placeholders", "list loading", "card loading"]
        },
        
        "dialog": {
          "description": "Confirmation and choice dialogs",
          "capability_level": "interactive",
          "typical_size": "4.2kb",
          "satisfaction_rate": 0.89,
          "features": {
            "action_buttons": { "essential": true, "description": "Clear action choices" },
            "escape_cancel": { "essential": true, "description": "Escape cancels action" },
            "focus_management": { "essential": true, "description": "Focus on primary action" }
          },
          "aria_requirements": ["role=alertdialog", "aria-labelledby", "aria-describedby"],
          "keyboard_support": ["Enter", "Escape", "Tab"],
          "use_cases": ["confirmations", "destructive actions", "important choices", "error dialogs"]
        }
      },
      
      "design_principles": [
        "Clear and timely communication",
        "Non-intrusive but noticeable",
        "Appropriate urgency indication",
        "Actionable when relevant",
        "Accessible to all users"
      ]
    }
  },
  
  "archetype_relationships": {
    "common_combinations": [
      {
        "pattern": "disclosure + input",
        "example": "modal + form",
        "usage_frequency": 0.56,
        "satisfaction": 0.89,
        "description": "Modal windows containing forms for data collection"
      },
      {
        "pattern": "collection + navigation",
        "example": "table + pagination",
        "usage_frequency": 0.42,
        "satisfaction": 0.87,
        "description": "Data tables with pagination controls"
      },
      {
        "pattern": "collection + collection",
        "example": "filter + results",
        "usage_frequency": 0.34,
        "satisfaction": 0.91,
        "description": "Filtering interface with results display"
      },
      {
        "pattern": "navigation + collection",
        "example": "tabs + content",
        "usage_frequency": 0.38,
        "satisfaction": 0.88,
        "description": "Tabbed interface organizing content sections"
      }
    ],
    
    "anti_patterns": [
      {
        "pattern": "disclosure + disclosure",
        "example": "modal inside modal",
        "reason": "Creates confusing focus management and escape paths",
        "alternative": "Use stepper pattern or single modal with multiple steps"
      },
      {
        "pattern": "multiple feedback",
        "example": "toast + alert + progress",
        "reason": "Overwhelming and competing for attention",
        "alternative": "Prioritize single most important status message"
      }
    ]
  },
  
  "evolution_metrics": {
    "success_indicators": [
      "User task completion rate",
      "Time to completion",
      "Error rate reduction",
      "Accessibility compliance score",
      "User satisfaction ratings"
    ],
    
    "optimization_targets": [
      "Reduce cognitive load",
